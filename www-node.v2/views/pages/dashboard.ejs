<!doctype html>
<!-- views/pages/index.ejs -->

<html lang="en">
<head>
	<% include ../partials/head %>
</head>
<body>

	<header>
        <% include ../partials/header %>
        <% include ../partials/sidebar %>
    </header>
	

        




        <!-- begin MAIN PAGE CONTENT -->
        <div id="page-wrapper">

            <div class="page-content">

                <!-- begin PAGE TITLE AREA -->
                <!-- Use this section for each page's title and breadcrumb layout. In this example a date range picker is included within the breadcrumb. -->
                <div class="row">
                    <div class="col-lg-12">
                        <div class="page-title">
                            <h1><%= pagetitle %>
                                <small>Content Overview</small>
                            </h1>
                            <ol class="breadcrumb">
                                <li class="active"><i class="fa fa-dashboard"></i> <%= pagetitle %></li>
                                <li class="pull-right">
                                    <div id="reportrange" class="btn btn-green btn-square date-picker">
                                        <i class="fa fa-calendar"></i>
                                        <span class="date-range"></span> <i class="fa fa-caret-down"></i>
                                    </div>
                                </li>
                            </ol>
                        </div>
                    </div>
                    <!-- /.col-lg-12 -->
                </div>
                <!-- /.row -->
                <!-- end PAGE TITLE AREA -->

                Unauthenticated Routes
                <ul>
                    <li><a href="/login">/login</a><br>
                    <li><a href="/logout">/logout</a><br>
                    <li><a href="/api">/api</a><br>
                    <li><a href="/logout">/api/authenticate</a><br>
                </ul>

                Authenticated Routes
                <ul>
                    <li><a href="/api/mongo/create" target='_blank'>/api/mongo/create</a><br>
                    <li><a href="/api/mongo/read">/api/mongo/read</a><br>
                    <li><a href="/api/mongo/read/prophetnite">/api/mongo/read/[username]</a><br>
                    <li><a href="/api/mongo/delete/_IDOID">/api/mongo/delete/_id</a><br>
                    <li><a href="/api/users" target='_blank'>/api/users</a><br>
                </ul>
            
            </div>
            <!-- /.page-content -->

        </div>
        <!-- /#page-wrapper -->
        <!-- end MAIN PAGE CONTENT -->

    



        



    <footer>
        <% include ../partials/footer %>
    </footer>


	<script>
		$(document).ready(function() {
			App.init();
			MapGoogle.init();
		});
	</script>

</body>
</html>

